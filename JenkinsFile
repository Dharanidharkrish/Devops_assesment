@Library('shlib')_
pipeline {
    agent any
    tools {
        maven "Maven"   
    }   
  
    stages {
        stage('Compile-Build-Test ') {
            steps {
	    	//slackNotify 'Building Started'
                build 'BUILD'
            }
	
        }
        stage('SonarQube Analysis'){
            steps{
               sonar()
            }
        }
	
	stage('Nexus') {
	 steps{
		nexus() 
		}
		post{
		/*success{
			slackNotify 'Nexus SUCCESS'
		}*/
	    	failure{
			jira 'NEXUS STAGE FAILED','FRI-4'
		}
	    }
	}
	stage('Deploy') {
	 steps{
		devEnv() 
		}
	    }
	
	
      }
	}

                build 'BUILD'
           	    }
		post
		{
		 failure { jira()}
		}
            }	
	 stage('Compile-Build-Test ') {
            steps {
	    		deploy adapters: [tomcat8(credentialsId: 'tomcat', path: '', url: 'http://15.206.159.129:8080')], contextPath: null, onFailure: false, war: '**/*.war'
           	    }
		}
      }
     }
